 @z
+
+@x
+      if (a->del>d && a->tip->valid==v)
+        if (a->tip==goal) last_arc=a;
+        else best_arc=a,d=a->del;
+@y
+      if (a->del>d && a->tip->valid==v) {
+        if (a->tip==goal) last_arc=a;
+        else best_arc=a,d=a->del;
+      }
+@z
